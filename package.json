{
  "name": "@electerious/eslint-config",
  "version": "5.2.1",
  "description": "ESLint configuration for my personal projects",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "homepage": "https://github.com/electerious/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/electerious/eslint-config.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./src/index.js",
    "./react": "./src/react.js"
  },
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "eslint": "eslint \"**/*.js\"",
    "format": "npm run eslint -- --fix && npm run prettier -- --write",
    "lint": "npm run eslint && npm run prettier -- --check",
    "prettier": "prettier --ignore-path .gitignore \"**/*.{js,json,md,yml}\"",
    "test": "npm run lint && node --test"
  },
  "dependencies": {
    "@eslint/js": "^9.33.0",
    "eslint": "^9.33.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsdoc": "^60.5.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-unicorn": "^61.0.2",
    "globals": "^16.3.0"
  },
  "devDependencies": {
    "@electerious/prettier-config": "^4.0.0",
    "prettier": "^3.6.2"
  },
  "peerDependencies": {
    "eslint": "^9.33.0"
  },
  "engines": {
    "node": ">=22"
  },
  "authors": [
    "Tobias Reich <tobias@electerious.com>"
  ]
}
