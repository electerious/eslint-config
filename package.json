{
  "name": "@electerious/eslint-config",
  "version": "4.1.0",
  "description": "ESLint configuration for my personal projects",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "homepage": "https://github.com/electerious/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/electerious/eslint-config.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "eslint": "eslint '**/*.js'",
    "format": "npm run eslint -- --fix && npm run prettier -- --write",
    "lint": "npm run eslint && npm run prettier -- --check",
    "prettier": "prettier --ignore-path .gitignore '**/*.{js,json,md,yml}'",
    "test": "npm run lint"
  },
  "dependencies": {
    "@eslint/js": "^9.33.0",
    "eslint": "^9.33.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^51.4.1",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "^16.3.0"
  },
  "devDependencies": {
    "@electerious/prettier-config": "^3.0.0",
    "prettier": "^3.6.2"
  },
  "peerDependencies": {
    "eslint": ">=9.33.0"
  },
  "engines": {
    "node": ">=22"
  },
  "authors": [
    "Tobias Reich <tobias@electerious.com>"
  ]
}
